if [ -z "$ENV_INITIALIZED" ] || [ "$ENV_INITIALIZED" -eq 0 ]; then
  export EDITOR=vim
  export LANG=en_US.UTF-8
  export DEFAULT_USER=omer
  export PATH="$HOME/.local/bin:$PATH"
  export ENV_INITIALIZED=1
fi

alias cp="cp -i"
alias rm="rm -i"
alias mv="mv -i"
if [[ "$TERM" =~ -256color$ ]]; then
  alias tmux='COLOR256=1 tmux'
fi
